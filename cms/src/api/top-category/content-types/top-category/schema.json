{
  "kind": "collectionType",
  "collectionName": "top_categories",
  "info": {
    "singularName": "top-category",
    "pluralName": "top-categories",
    "displayName": "TopCategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "preview": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "seo.title-description"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "top_category"
    }
  }
}
